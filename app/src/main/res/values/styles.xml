<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="spoonRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/spoon_rating_bar</item>
        <item name="android:minHeight">64dp</item>
        <item name="android:maxHeight">64dp</item>
    </style>

    <style name="add_dish_popup_animation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>
</resources>